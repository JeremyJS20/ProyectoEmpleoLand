<%- include("template/cabecera", {tituloWeb: 'Mi perfil'}) %>
<div class="container p-0 mt-3 mb-3">
    <div class="row m-0 ">
        <div class="col-md-9 pt-2 pb-2 shadow">
            <div class="bg-transparent pt-5 pb-5 text-white"
                style="background-image: url(<%=Images.ProfileBackground%>); background-size: cover;">
                <div class="row justify-content-center">
                    <div class="col-md-6 text-center">
                        <img src="<%= Images.ProfileLogo %>" class="rounded-circle" alt="ddd" style="width:40%; height:auto">
                        <%if(req.user != undefined && req.user.userType === 'Company'){%> <button
                            class="btn btn-primary btn-circle " style="  position: absolute; top: 85%; left: 63%; transform: translate(-50%, -50%);
                            -ms-transform: translate(-50%, -50%);cursor: pointer;" id="btnAddPhoto">
                        <i class="fas fa-camera"></i>
                        </button>
                        <%}%>
                    </div>
                    <div class="col-md-12 text-center h2 font-weight-normal">
                        <%= CompanyData.Nombre %>
                    </div>
                </div>
            </div>

            <div class="mt-0">
                <div class="card">
                    <div class="card-header">
                        <%if(req.user != undefined && req.user.userType === 'Company'){%>
                            <ul class="nav nav-pills card-header-pills justify-content-center">
                                <li class="nav-item">
                                    <a
                                        class="nav-link <%if(req.params.tab === undefined || req.params.tab === 'about'){%> active <%}%>" href="/myProfile-CompanyID=<%= req.params.IDUser %>/about">Acerca de</a>
                                </li>
                                <li class="nav-item">
                                    <a
                                        class="nav-link <%if(req.params.tab === 'recentsEmploys'){%> active <%}%>" href="/myProfile-CompanyID=<%= req.params.IDUser %>/recentsEmploys">Empleos recientes</a>
                                </li>
                                <li class="nav-item">
                                    <a
                                        class="nav-link <%if(req.params.tab === 'peopleLinked'){%> active <%}%>" href="/myProfile-CompanyID=<%= req.params.IDUser %>/peopleLinked">Personas vinculadas</a>
                                </li>
                            </ul>
                        <%} else{%>
                            <ul class="nav nav-pills card-header-pills justify-content-center">
                                <li class="nav-item">
                                    <a
                                        class="nav-link <%if(req.params.tab === undefined || req.params.tab === 'about'){%> active <%}%>" href="/companyProfile-CompanyID=<%= req.params.IDUser %>/about">Acerca de</a>
                                </li>
                                <li class="nav-item">
                                    <a
                                        class="nav-link <%if(req.params.tab === 'recentsEmploys'){%> active <%}%>" href="/companyProfile-CompanyID=<%= req.params.IDUser %>/recentsEmploys">Empleos recientes</a>
                                </li>
                                <li class="nav-item">
                                    <a
                                        class="nav-link <%if(req.params.tab === 'peopleLinked'){%> active <%}%>" href="/companyProfile-CompanyID=<%= req.params.IDUser %>/peopleLinked">Personas vinculadas</a>
                                </li>
                            </ul>
                        <%}%>
                    </div>
                    <div class="card-body">
                        <%if(req.params.tab === undefined || req.params.tab === 'about'){%>
                            <div class="row g-3 justify-content-between">
                                <div class="col-md-12">
                                    <%if(CompanyData2.Resumen === '' || CompanyData2.Resumen === null){%>
                                    <%if(req.user != undefined && req.user.userType === 'Company'){%>
                                    <div class="text-center">
                                        <img width="100" height="100" src="data:image/svg+xml;base64," />
                                        <p>
                                            Haz un breve resumen de su empresa.
                                        </p>
                                    </div>
                                    <div class="text-center">
                                        <button class="btn text-primary" id="btnSummary">
                                            <i class="far fa-plus"></i> Añadir
                                        </button>
                                    </div>
                                    <%}%>
                                    <%} else{%>
                                        <div class="card mb-4">
                                            <div class="row m-0">
                                                <div class="col-md-3 text-center bg-primary">
                                                    <div class="card-body text-white" style="position: relative; top: 50%; -webkit-transform: translateY(-50%);
                                                            -ms-transform: translateY(-50%); transform: translateY(-50%);">
                                                        <div class="h5 font-weight-normal">Resumen</div>
                                                    </div>
                                                </div>
                                                <div class="col-md-9">
                                                    <div class="card-body">
                                                        <div id="summaryContainer" class="text-justify col-md-12">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    <%if(req.user != undefined && req.user.userType === 'Company'){%>
                                    <div class="col-md-12 text-center row">
                                        <button class="btn text-primary col-6" id="btnSummary">
                                            <i class="fal fa-edit"> </i> Editar
                                        </button>
                                        <form action="/defineSummary/redirectTo=<%= req.originalUrl.replaceAll('/', '*') %>" class="col-6" method="POST">
                                            <button class="btn text-danger" type="submit">
                                                <i class="fal fa-trash-alt"> </i> Borrar
                                            </button>
                                        </form>
                                    </div>
                                    <%}%>
                                <%}%>
                                </div>
                                <div class="col-md-12">
                                    <%if(CompanyData2.Mision === '' || CompanyData2.Mision === null){%>
                                    <%if(req.user != undefined && req.user.userType === 'Company'){%>
                                    <div class="text-center">
                                        <img width="100" height="100" src="data:image/svg+xml;base64," />
                                        <p>
                                            Defina la Mision de su empresa.
                                        </p>
                                    </div>
                                    <div class="text-center">
                                        <button class="btn text-primary" id="btnMision">
                                            <i class="far fa-plus"></i> Añadir
                                        </button>
                                    </div>
                                    <%}%>
                                    <%} else{%>
                                        <div class="card mb-4">
                                            <div class="row m-0">
                                                <div class="col-md-3 text-center bg-primary">
                                                    <div class="card-body text-white" style="position: relative; top: 50%; -webkit-transform: translateY(-50%);
                                                            -ms-transform: translateY(-50%); transform: translateY(-50%);">
                                                        <div class="h5 font-weight-normal">Mision</div>
                                                    </div>
                                                </div>
                                                <div class="col-md-9">
                                                    <div class="card-body">
                                                        <div id="missionContainer" class="text-justify col-md-12">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    <%if(req.user != undefined && req.user.userType === 'Company'){%>
                                    <div class="col-md-12 text-center row">
                                        <button class="btn text-primary col-6" id="btnMision">
                                            <i class="fal fa-edit"> </i> Editar
                                        </button>
                                        <form action="/defineMision/redirectTo=<%= req.originalUrl.replaceAll('/', '*') %>" class="col-6" method="POST">
                                            <button class="btn text-danger" type="submit">
                                                <i class="fal fa-trash-alt"> </i> Borrar
                                            </button>
                                        </form>
                                    </div>
                                    <%}%>
                                <%}%>
                                </div>
                                <div class="col-md-12">
                                    <%if(CompanyData2.Vision === '' || CompanyData2.Vision === null){%>
                                    <%if(req.user != undefined && req.user.userType === 'Company'){%>
                                    <div class="text-center">
                                        <img width="100" height="100" src="data:image/svg+xml;base64," />
                                        <p>
                                            Defina la vision de su empresa.
                                        </p>
                                    </div>
                                    <div class="text-center">
                                        <button class="btn text-primary" id="btnVision">
                                            <i class="far fa-plus"></i> Añadir
                                        </button>
                                    </div>
                                    <%}%>
                                    <%} else{%>
                                        <div class="card mb-4">
                                            <div class="row m-0">
                                                <div class="col-md-3 text-center bg-primary">
                                                    <div class="card-body text-white" style="position: relative; top: 50%; -webkit-transform: translateY(-50%);
                                                            -ms-transform: translateY(-50%); transform: translateY(-50%);">
                                                        <div class="h5 font-weight-normal">Vision</div>
                                                    </div>
                                                </div>
                                                <div class="col-md-9">
                                                    <div class="card-body">
                                                        <div id="visionContainer" class="text-justify col-md-12">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    <%if(req.user != undefined && req.user.userType === 'Company'){%>
                                    <div class="col-md-12 text-center row">
                                        <button class="btn text-primary col-6" id="btnVision">
                                            <i class="fal fa-edit"> </i> Editar
                                        </button>
                                        <form action="/defineVision/redirectTo=<%= req.originalUrl.replaceAll('/', '*') %>" class="col-6" method="POST">
                                            <button class="btn text-danger" type="submit">
                                                <i class="fal fa-trash-alt"> </i> Borrar
                                            </button>
                                        </form>
                                    </div>
                                    <%}%>
                                <%}%>
                                </div>
                                <div class="col-md-12 mt-2 text-center text-white row m-0 bg-primary pt-2 pb-2">
                                    <div class="col-6">
                                        <p class="font-weight-bold">
                                            Area:
                                        </p>
                                        <p class="font-weight-bold">
                                            <%console.log(employs)%>
                                            Email:
                                        </p>
                                        <p class="font-weight-bold">
                                            Telefono:
                                        </p>
                                        <p class="font-weight-bold">
                                            URL:
                                        </p>
                                        <p class="font-weight-bold">
                                            Direccion:
                                        </p>
                                    </div>
                                    <div class="col-6">
                                        <p class="">
                                            <%= CompanyData.Area %>
                                        </p>
                                        <p class="">
                                            <%= CompanyData.Email %>
                                        </p>
                                        <p class="">
                                            <%= CompanyData.Telefono %>
                                        </p>
                                        <p>
                                            <a href="<%= CompanyData.URL %>" class="text-white">
                                                <%= CompanyData.URL %>
                                            </a>
                                        </p>
                                        <p class="">
                                            <%= CompanyData.Calle  %> <%= CompanyData.NumCasa  %>, <%= CompanyData.Dm  %> - <%= CompanyData.Municipio  %>, 
                                            <%= CompanyData.Provincia  %>
                                        </p>
                                    </div>
                                </div>
                            </div>
                        <%} else if(req.params.tab === 'recentsEmploys') { 
                            if(employs.length != 0){%>
                                    <%for(let i=0; i < employs.length; i++){%>
                                        <div class="card mb-4 p-2 bg-light">
                                            <div class="row mb-2 m-0 align-items-center">
                                                <div class="col-md-auto ">
                                                  <a href="<%= req.originalUrl %>/<%=employs[i].IDTrabajo%>"
                                                    class="h5 mr-3 p-0 text-decoration-none font-weight-normal text-dark"><%= employs[i].Nombre %> |</a>
                                                </div>
                                                <div class="col-md-auto bg-primary text-white rounded mr-1 pl-1 pr-1 mb-1" style="height: 25px; width: auto">
                                                  <p class=""><%= employs[i].TipoTrabajo %></p>
                                                </div>
                                                <div class="col-md-auto bg-secondary text-white rounded mr-1 pl-1 pr-1 mb-1" style="height: 25px; width: auto">
                                                  <p class=""><%= employs[i].Tanda %></p>
                                                </div>
                                                <div class="col-md-auto bg-success text-white rounded pl-1 pr-1 mb-1" style="height: 25px; width: auto">
                                                  <p class=""><%= employs[i].Modalidad %></p>
                                                </div>
                                              </div>
                                      
                                              <div class="row m-0 text-muted">
                                      
                                                <div class="col-md-auto p-0 mr-3">
                                                  <i class="fal fa-map-marker-alt text-dark font-weight-bold"></i> <%=employs[0].Calle %>
                                                  , <%= employs[0].Dm %>, <%= employs[0].Municipio %> - <%= employs[i].Provincia %>
                                                </div>
                                                <div class="col-md-auto p-0">
                                                  <i class="fal fa-clock text-dark font-weight-bold"></i> <%= employs[i].FechaDePub %>
                                                </div>
                                              </div>
                                        </div>
                                    <%
                                    if(i === 4){
                                        break;
                                    }}%>
                        <%} else{%>
                            <div class="text-center">
                                <p>
                                    Todavia no ha publicado ningun empleo.
                                </p>
                            </div>
                            <div class="text-center">
                                <a href="/postEmploy" class="btn text-primary">
                                    <i class="far fa-share-square"></i> Publicar empleo
                                </a>
                            </div>
                            <%}
                        } else{%>

                        <%}%>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

